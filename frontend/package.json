{
    "name": "ecommerce-frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "packageManager": "pnpm@8.15.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
